{
  "hosting": {
    "public": "docs",
    "ignore": [
      "**.md",
      "**.sh",
      "**/.*",
      "**/.git/**",
      "**/node_modules/**",
      "firebase.json",
      "index.src/**",
      "LICENSE",
      "samples/**",
      "src/**"
    ],
    "rewrites": [
      {
        "source": "/**/",
        "function": "index"
      }
    ]
  }
}

